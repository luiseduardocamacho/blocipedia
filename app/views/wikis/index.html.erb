<h1>All Posts</h1>
<div class="row">
  <div class="col-md-8">
      <div class="media">
        <div class="media-body">
          <% @wikis.each do |wiki| %>
            <h4 class="media-heading">
              <%= link_to wiki.title, wiki %>
              <% end %>
            </h4>
        </div>
      </div>
    </div>

  <div class="col-md-4">
    <%= link_to "New Wiki", new_wiki_path, class: 'btn btn-danger' %>
  </div>
</div>
